{
  "schemaVersion": 1,
  "id": "capejs",
  "version": "${version}",
  "name": "CapeJS",
  "description": "Allows you to give users custom capes through KubeJS",
  "authors": [
    "static"
  ],
  "contact": {
    "homepage": "https://github.com/TheStaticVoid/CapeJS",
    "sources": "https://github.com/TheStaticVoid/CapeJS",
    "issues": "https://github.com/TheStaticVoid/CapeJS/issues"
  },
  "license": "MIT",
  "icon": "assets/capejs/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "dev.thestaticvoid.capejs.CapeJS"
    ]
  },
  "mixins": [
    "capejs.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": ">=0.76.1",
    "minecraft": "${minecraft_version}",
    "kubejs": ">=1902.6.1-build.300"
  }
}
