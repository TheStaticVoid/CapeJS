{
  "schemaVersion": 1,
  "id": "capejs",
  "version": "${version}",

  "name": "CapeJS",
  "description": "Mod utilizing KubeJS to add custom capes to players using scripts",
  "authors": [
    "static"
  ],
  "contact": {
    "homepage": "http://github.com/TheStaticVoid/CapeJS",
    "issues": "http://github.com/TheStaticVoid/CapeJS/issues",
    "repo": "https://github.com/TheStaticVoid/CapeJS"
  },

  "license": "MIT",
  "icon": "assets/capejs/icon.png",

  "environment": "client",
  "entrypoints": {
    "client":[
      "dev.thestaticvoid.capejs.fabric.CapeJSFabric"
    ]
  },
  "mixins": [
    "capejs.mixins.json",
    "capejs-common.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.22",
    "minecraft": ">=1.20.1",
    "kubejs": ">=2001.6.3-build.34"
  }
}
